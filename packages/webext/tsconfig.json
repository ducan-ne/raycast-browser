{
  "extends": "plasmo/templates/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-jsxdev",
    "lib": [
      "esnext",
      "dom"
    ],
    "baseUrl": ".",
    "module": "esnext",
    "moduleResolution": "NodeNext",
    "allowJs": true,
    "paths": {},
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "types": [
      "@types/node",
      "@types/chrome"
    ],
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    ".plasmo/index.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
