{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-jsxdev",
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "NodeNext",
    "paths": {
    },
    "resolveJsonModule": true,
    "types": [
      "@types/node",
      "@types/chrome"
    ],
    "allowJs": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["*.ts", "*.tsx"]
}
