{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-browser",
  "title": "Raycast browser",
  "description": "Hello world template for Raycast browser extensions",
  "icon": "command-icon.png",
  "author": "an_",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Chrome's tabs",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/chrome": "^0.0.258",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "raycast-browser": "workspace:*",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}